{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@ant-design/pro-layout": "^6.3.0",
    "@tinymce/tinymce-react": "^3.5.0",
    "@types/qs": "^6.9.0",
    "@types/tinymce": "^4.5.24",
    "@umijs/preset-react": "1.x",
    "@umijs/route-utils": "^1.0.17",
    "@umijs/test": "^3.2.3",
    "antd": "^4.3.0",
    "bizcharts": "^3.5.6",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-amap": "^1.2.8",
    "react-amap-plugin-autocomplete": "0.0.4",
    "react-dom": "^16.12.0",
    "umi": "^3.2.3",
    "umi-request": "^1.0.8",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
